<%% import "github.com/qorpress/benchmarks/go-template/model" %%>
<%! func EgoSimple(w io.Writer, u *model.User) error %>
<html>
    <body>
        <h1><%= u.FirstName %></h1>

        <p>Here's a list of your favorite colors:</p>
        <ul>
        <% for _, colorName := range u.FavoriteColors { %>
            <li><%= colorName %></li><% } %>
        </ul>
    </body>
</html>